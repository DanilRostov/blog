{{!< default}}

{{!-- Header --}}
{{> "header-post"}}

{{!-- Navigation --}}
{{> "navigation-header"}}

{{#post}}

<main class="article">
    <div class="article__container">
        <h2 class="article__title">{{title}}</h2>
        <span class="article__date">{{date}}</span>
        <div class="article__img-box">
            <img src="{{feature_image}}" alt="">
        </div>
        <div class="article__content">
            {{content}}
        </div>
    </div>
</main>


{{/post}}